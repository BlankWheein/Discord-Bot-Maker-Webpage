<!doctype html>
<html>

<head>
  <title>Command Creator</title>
  <meta http-equiv="content-type" content="text/html;charset=utf-8" />
  <meta name="viewport"
    content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=no" />

  <link rel="stylesheet" href="dist/css/jsplumb/jsplumbtoolkit-defaults.css">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">

  <link rel="stylesheet" href="css/panels.css">
  <link rel="stylesheet" href="css/state-machine.css">
</head>

<body>
  <div class="jtk-main">
    <div class="panels">
      <div class="canvas-panel jtk-canvas canvas-wide statemachine jtk-surface jtk-surface-nopan" id="canvas"></div>
      <div class="edit-panel-holder">
        <span class="block-editor-title">Block Editor</span>
        <hr />
        <div class="edit-panel" id="edit-panel"></div>
      </div>
    </div>

    <br />
    <input type="button" class="btn btn-outline-dark" id="add-send-message" value="Add 'Send Message'">
    <input type="button" class="btn btn-outline-dark" id="add-if-statement" value="Add 'If Statement'">
    <input type="button" class="btn btn-outline-dark" id="add-on-ready" value="Add 'On Ready'">
    <input type="button" class="btn btn-outline-dark" id="add-set-var" value="Add 'Set Var'">
    <input type="button" class="btn btn-outline-dark" id="add-set-channel" value="Add 'Set Channel'">

    <br /><br />
    <input type="button" class="btn btn-success" id="generate-code" value="Parse into Json">

    <br />
    <div id="json-output"></div>
  </div>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jsPlumb/2.14.3/js/jsplumb.min.js"></script>

  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>

  <script src="js/util/key_controller.js"></script>

  <script src="js/blocks/block.js"></script>

  <script src="js/blocks/event_block.js"></script>
  <script src="js/blocks/events/on_ready_block.js"></script>

  <script src="js/blocks/action_block.js"></script>
  <script src="js/blocks/actions/send_message_block.js"></script>
  <script src="js/blocks/actions/set_variable_block.js"></script>
  <script src="js/blocks/actions/set_channel_block.js"></script>

  <script src="js/blocks/structure_block.js"></script>
  <script src="js/blocks/structures/decision_block.js"></script>

  <script src="js/canvas/canvas.js"></script>
  <script src="js/canvas/object_drawer.js"></script>

  <script src="js/parser/sequence_parser.js"></script>

  <script src="js/main.js"></script>
</body>

</html>