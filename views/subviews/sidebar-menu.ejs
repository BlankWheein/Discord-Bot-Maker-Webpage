<div class="sidebar-sticky pt-3">
  <% var active_class = (page_type == "home") ? "active" : "" %>
  <a class="nav-link <%= active_class %>" href="/">Home</a>
  <h6 class="sidebar-heading d-flex justify-content-between align-items-center px-3 mt-4 mb-1 text-muted">
    <span>Commands</span>
    <a class="d-flex align-items-center text-muted" id="add-command">
      <i class="fa fa-plus" aria-hidden="true"></i>
    </a>
  </h6>
  <ul class="nav flex-column mb-2">
    <% command_names.forEach(name => { %>
      <li class="nav-item">
        <% var active_class = (name == current_command) ? "active" : "" %>
        <a class="nav-link <%= active_class %>" href="/commands/<%= name %>">
          <%= name %>
        </a>
      </li>
    <% }); %>
  </ul>
</div>